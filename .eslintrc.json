{
  "extends": ["eslint:recommended", "standard", "plugin:lit/recommended","plugin:literate-comments/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "html",
    "lit",
    "literate-comments"
  ],
  "rules": {
    "lit/no-legacy-template-syntax": "off",
    "comma-dangle": "off"
  },
  "globals": {
  }
}
